<?php 
namespace Gesnotes\AssignaturaBundle\Entity;
use Doctrine\ORM\Mapping as ORM;


/**
* @ORM\Entity
*/
class Assignatura{
  
  /** 
   * @ORM\Id 
   * @ORM\Column(type="integer")
   * @ORM\GeneratedValue
   */
  protected $id;
  
  /** @ORM\Column(type="string", length=100) */
  protected $nom;
  
  /** @ORM\Column(type="string", length=100) */
  protected $slug;
  
}



?>
